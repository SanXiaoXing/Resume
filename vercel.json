{
  "routes": [
    { 
      "src": "/(.*\\.(svg|css|js|json|html))", 
      "headers": { "Cache-Control": "public, max-age=3600" },
      "dest": "/$1" 
    },
    
    { 
      "src": "/Archived/(.*)", 
      "dest": "/Archived/$1" 
    },
    
    { 
      "src": "/archived", 
      "dest": "/archived.html",
      "headers": { "Content-Type": "text/html" }
    },
    { 
      "src": "/", 
      "dest": "/index.html",
      "headers": { "Content-Type": "text/html" }
    },
    
    { 
      "src": "/(.*)", 
      "dest": "/index.html",
      "headers": { "Content-Type": "text/html" }
    }
  ]
}