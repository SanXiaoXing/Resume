{
  "routes": [
    { 
      "src": "/Archived/(.*)", 
      "dest": "/Archived/$1" 
    },
    
    { 
      "src": "/archived", 
      "dest": "/archived.html",
      "headers": { "Content-Type": "text/html" }
    },
    { 
      "src": "/", 
      "dest": "/index.html",
      "headers": { "Content-Type": "text/html" }
    }
  ]
}