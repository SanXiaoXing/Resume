{
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" },
      {
        "src": "/archived",
        "dest": "/archived.html"
      },
      {
        "src": "/Archived/(.*)",
        "dest": "/Archived/$1"
      }
    ],
    "trailingSlash": false,
    "cleanUrls": true
  }